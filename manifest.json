{
  "manifest_version": 2,

  "name": "Simple Notes",
  "description": "A simple notes extension based off the Mac Notes app.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },

  "permissions": [
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
